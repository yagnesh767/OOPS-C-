#include <iostream>
using namespace std;
double computeAverage(int a, int b, int c) {
    double average = (a + b + c) / 3.0;
    return average;
}

int main() {
    int a, b, c;
    cout << "Enter three integers: ";
    cin >> a >> b >> c;
    double avg = computeAverage(a, b, c);
    cout << "The average of " << a << ", " << b << ", and " << c << " is: " << avg << endl;

    return 0;
}
